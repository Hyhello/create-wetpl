{
    "name": "create-wetpl",
    "version": "0.1.0",
    "description": "A Cli Tool for weTpl",
    "type": "module",
    "bin": {
        "create-wetpl": "lib/index.mjs"
    },
    "scripts": {
        "prepare": "husky install",
        "prebuild": "rimraf lib",
        "release": "release-it",
        "build": "node ./scripts/build.mjs",
        "lint": "tsc --noEmit && eslint --ext .ts src"
    },
    "keywords": [
        "create-wetpl",
        "wetpl"
    ],
    "author": "Hyhello",
    "license": "MIT",
    "homepage": "https://github.com/Hyhello/create-wetpl.git",
    "bugs": {
        "url": "https://github.com/Hyhello/create-wetpl/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Hyhello/create-wetpl.git"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "lint-staged": {
        "src/**/*.{ts}": [
            "eslint --fix"
        ]
    },
    "files": [
        "lib"
    ],
    "directories": {
        "lib": "lib"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "devDependencies": {
        "@release-it/conventional-changelog": "^5.0.0",
        "@types/prompts": "^2.4.2",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "esbuild": "^0.20.2",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "giget": "^1.1.3",
        "husky": "^7.0.4",
        "lint-staged": "^14.0.1",
        "prettier": "^3.2.5",
        "prompts": "^2.4.2",
        "release-it": "^13.1.1",
        "rimraf": "^4.4.1",
        "typescript": "^4.5.5"
    }
}
